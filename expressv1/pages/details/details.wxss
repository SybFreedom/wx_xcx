.detail_tip{line-height: 80rpx;font-size: 16px;color: #4569FF;display: flex;flex-wrap: wrap}
.detail_tip text:first-of-type{margin-right: 32px}
.detail_tip view:last-of-type{margin-left: 30rpx;display: flex;flex-wrap: wrap}
.bottom_status{height: max-content;background-color: #ffffff}
.bottom_status>view{height: max-content;border-bottom:1px solid #F0F0F0;display: flex;flex-direction: column;padding-bottom: 23rpx}
.bottom_status>view>view:first-of-type{height: 43rpx;margin-top: 23rpx;display: flex;align-items: center;justify-content:space-between}
.bottom_status>view>view:first-of-type>view:first-of-type{margin-left: 30rpx;display: flex;align-items: center;}
.bottom_status>view>view:first-of-type>view:first-of-type text{font-size: 28rpx;color: #006cff;width: max-content}
.bottom_status>view>view:first-of-type>view:last-of-type{margin-right: 30rpx;font-size: 24rpx;border-radius: 22rpx;height: 43rpx;line-height: 43rpx;text-align: center;color:#fff;width: 100rpx}
.circle{height: 33rpx;width: 33rpx;background-color: #006cff;border-radius: 100%;margin-right: 25rpx}
.bottom_status_time{display: flex;align-items: center;margin-top: 13rpx;color: #696969;margin-left: 88rpx}
.bottom_status_time>text:first-of-type{font-size: 22rpx}
.bottom_status_time>text:last-of-type{font-size: 22rpx;font-weight: bold}