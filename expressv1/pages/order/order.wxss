/* pages/order/order.wxss *//* pages/discount/discount.wxss */

  page {
  background-color: #F5F5F5;
}
.nav {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #EEEEEE;
  background-color: #ffffff;
}
.default {
  line-height: 80rpx;
  text-align: center;
  /* width: 200rpx; */
  color: #8E8E8E;
  font-size: 28rpx;
  margin: 0 50rpx;
}
.red {
  line-height: 80rpx;
  text-align: center;
  color: #222222;
  /* width: 200rpx; */
  margin: 0 50rpx;
  font-size: 28rpx;
  border-bottom: 2px solid #FFD428;
}
.show {
  display: block;
  /* text-align: center;
  line-height: 200rpx; */
}
.hidden {
  display: none;
  text-align: center;
  line-height: 200px;
}

.father{
  width: 100%;
}
.father .son{
  width: 90%;
  margin: auto 5%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 20rpx;
  border-bottom: 2px solid #f5f5f5;
}
.father .son text:nth-child(1){
  text-align: left;
  color: #bfbfbf;
  font-size: 16px;
  width: 30%;
  border: 1px solid blue;
}
.father .son text:nth-child(2){
  margin-left: 30rpx;
  font-size: 16px;
  white-space: pre-wrap;
}

.father .son view{
  /* margin-left: 60rpx; */
  display: inline-block;
  width: 70%;
  border: 1px solid red;
  height: 100%;
  font-size: 16px;
  white-space: pre-wrap;
}
/* .address{
  width: 80%;
  display: inline-block;
  border: 1px solid red;
  overflow: visible;
} */

.item{
  display: flex;
  flex-direction: column;
  width: 94%;
  padding: 35rpx 3%;
  background-color: #ffffff;
}
.item>view:first-of-type{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
}
.item>view:first-of-type>view{
  display: flex;
  flex-direction: column;

}
.item>view:first-of-type>view>text:first-of-type{
  font-size: 16px;
  color: #333333;
}
.item>view:first-of-type>view>text:last-of-type{
  font-size: 14px;
  color: #ABABAB;
}
.item>view:first-of-type>text{
  font-size: 14px;
  color: #666;
}
.item>view:nth-of-type(2){
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin: 30rpx 0 15rpx;
}
.item>view:nth-of-type(2)>view{
  display: flex;
  flex-direction: row;
  flex:1;
  align-items: center;
  justify-content: space-between;

}
.item>view:nth-of-type(2)>view>text:first-of-type{
  padding: 10rpx 30rpx;
  border-radius: 60rpx;
  background-color: #0078FF;
  color: #ffffff;
  font-size: 28rpx;
}
.item>view:nth-of-type(2)>view>text:last-of-type{
  font-size: 24rpx;
  color: #444444;
  flex:1;
  text-align: center;
}
.item>view:nth-of-type(2)>text{
  font-size: 28rpx;
  width: 40%;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.item>view:nth-of-type(3){
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin: 15rpx 0;
}
.item>view:nth-of-type(3)>view{
  display: flex;
  flex-direction: row;
  flex: 1;
  align-items: center;
  justify-content: space-between;

}
.item>view:nth-of-type(3)>view>text:first-of-type{
  padding: 10rpx 30rpx;
  border-radius: 60rpx;
  background-color: #FF6A28;
  color: #ffffff;
  font-size: 28rpx;
}
.item>view:nth-of-type(3)>view>text:last-of-type{
  font-size: 24rpx;
  color: #444444;
  flex:1;
  text-align: center;
}
.item>view:nth-of-type(3)>text{
  font-size: 28rpx;
  width: 40%;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.item>view:nth-of-type(4){
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 40rpx;
}
.item>view:nth-of-type(4)>view:first-of-type{
  display: flex;
  flex-direction: row;
  flex: 1;
  align-items: center
}
.item>view:nth-of-type(4)>view:first-of-type>view:first-of-type{
  height: 24rpx;
  width: 24rpx;
  border-radius: 50%;
  background-color: rgb(41, 240, 41);
}
.item>view:nth-of-type(4)>view:first-of-type>text:first-of-type{
  font-size: 28rpx;
  color: rgb(41, 240, 41);
  margin-left: 10rpx;
}
.item>view:nth-of-type(4)>view:first-of-type>view:last-of-type{
  height: 24rpx;
  width: 24rpx;
  border-radius: 50%;
  background-color: #FF6A28;
}
.item>view:nth-of-type(4)>view:first-of-type>text:last-of-type{
  font-size: 28rpx;
  color: #FF6A28;
  margin-left: 10rpx;
}
.item>view:nth-of-type(4)>view:nth-of-type(2){
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 24rpx;
  border:1px solid #0078ff;
  background-color: transparent;
  width: 193rpx;
  height: 48rpx;
}
.item>view:nth-of-type(4)>view:nth-of-type(2)>text{
  font-size: 32rpx;
  color: #0078ff;
}
.item>view:nth-of-type(4)>view:nth-of-type(3){
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 24rpx;
  border:1px solid #ffd428;
  background-color: #ffd428;
  width: 193rpx;
  height: 48rpx;
  margin-left: 2rpx;
}
.item>view:nth-of-type(4)>view:nth-of-type(3)>text{
  font-size: 32rpx;
  color: #222;
}
.item_tip{width: 100%;height: 90rpx;margin-top: 1px;background-color: #fff;margin-bottom: 14rpx;display: flex;align-items: center;}
.item_tip>text:first-of-type{color: #2f2f2f;font-size: 22rpx;margin-left: 29rpx;width: 700rpx}
.item_tip>view:first-of-type{display: flex;align-items: center;width: 300rpx;left:563rpx;height: 90rpx;justify-content: flex-end;margin-right: 20rpx}
.item_tip>view:first-of-type>view{height: 33rpx;width: 33rpx;border-radius: 100%;}
.item_tip>view:first-of-type>text{font-size: 28rpx;margin-left: 10rpx}