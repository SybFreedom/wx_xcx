/* pages/commentOrder/commentOrder.wxss */
page{
  background-color: white;
}
.textarea{
  width: auto;
  font-size: 14px;
  height: 150px;
  border-bottom: 1px solid #ececec;
  margin: 10px;
}

.evalueBgView{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12px;
  font-size: 14px;
}

.evalueDes{
  margin-left: 10px;
}

.startBgView{
  display: flex;
}

.evalueIcon{
  margin-right: 10px;
  width: 20px;
  height: 20px;
}

.commitBt{
  margin-top: 15px;
  background-color: #FFD428;
  color: #333;
  margin-left: 15px;
  margin-right: 15px;
}
.uploadimg{
  width: 94%;
  padding: 0 3%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.nullimg{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.blockimg{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.blockimg>view{
  width: 150rpx;
  height: 150rpx;
  margin: 10rpx;
  position: relative;
}
.blockimg>view>image{
  width: 100%;
  height: 100%;
}

.blockimg>view>text{
  width: 40rpx;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  background-color: #FF4900;
  color: #ffffff;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.openimg{
  width: 167rpx;
  height: 167rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #efefef;
  margin-right: 20rpx;
}
.openimg>image{
  width: 100rpx;
}
.nullimg>view:last-of-type>text{
  font-size: 14px;
  color: #666;
}
