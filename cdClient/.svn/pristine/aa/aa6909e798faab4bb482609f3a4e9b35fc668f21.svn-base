.topAddressView {
  /* background-color: white; */
  display: flex;
  align-items: center;
}
checkbox:first-of-type{
  -webkit-appearance: none;
  width: 25rpx;
  height: 25rpx;
  border-radius: 15rpx;
  position: absolute;
  left: 5rpx;
  top:25rpx;
}
checkbox:last-of-type{
  -webkit-appearance: none;
  width: 30rpx;
  height: 30rpx;
  border-radius: 15rpx;
  position: absolute;
  left: 5rpx;
  top: 145rpx
}
.unselectedAddress {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 10px;
  background-color: #ffffff;
  padding-left: 30rpx;
  position: relative
}

.selectedAddress {
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 15px;
  margin-right: 15px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 14px;
  color: #303030;
}

.selectedAddress  .left {
  display: flex;
  flex-direction: column;
}

.nameAndTelView {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.selectedAddress .name {
  margin-right: 10px;
}

.selectedAddress .icon {
  width: 16px;
  height: 16px;
  margin-right: 6px;
}

.tel {
}

.detailAddress {
  margin-top: 8px;
}

.addressIcon {
  margin-left: 20px;
  width: 18px;
  height: 20px;
}

.addressText {
  margin-left: 20px;
  width: 80%;
  font-size: 12px;
  color: #999;
}

.addressChooseImg {
  margin-right: 10px;
  width: 12px;
  height: 12px;
}

.topCutView {
  display: block;
  height: 2px;
  width: 100%;
}

.sectionHeaderView {
  margin-top: 10px;
  background-color: white;
  display: flex;
  flex-direction: column;
}

.sectionHeaderViewIcon {
  margin-left: 16px;
  margin-top: 8px;
  margin-bottom: 8px;
  margin-right: 6px;
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

.sectionHeaderViewText {
  color: #303030;
  font-size: 14px;
  /* margin-top: 14px; */
}

.goodsListView {
  display: flex;
  margin-top: 0.5px;
  background-color: white;
  flex-direction: column;
}

.goodsItemViewWrap{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.goodsItemView {
  margin-left: 15px;
  margin-right: 15px;
  margin-top: 8px;
  margin-bottom: 8px;
  display: flex;
  flex-direction: row;
  font-size: 12px;
  flex-wrap: wrap;
  justify-content: space-between;
}
.goodsItemView2>image{
  width: 15px;
  margin-right: 3px;
}
.goodsItemView2{
  margin: 0 15px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 10px 0;
}
.goodsItemView2>view{
  font-size: 14px;
}
.goodsItemView>view{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
}
.goodsItemView>view:first-of-type>image{
  width: 70px;
  border-radius: 3px;
}
.goodsItemView>view:nth-of-type(2){
  width: 70%;
}
.goodsItemView>view:last-of-type>image{
  width: 7px;
}

.goodsItemText1 {
  /* width: 60%; */
  color: #303030;
}

.goodsItemText2 {
  color: #303030;
}

.goodsItemText3 {
  color: #ec6d68;
}

.cutView {
  height: 1px;
  background-color: #ececec;
}

.goodsSectionFooterView {
  display: flex;
  justify-content: flex-end;
  margin-right: 15px;
  margin-top: 10px;
  margin-bottom: 10px;
  font-size: 14px;
  color: #111;
}

.footerText1 {
  margin-right: 8px;
  color: #303030;
}

.note {
  padding: 0 3%;
  width: 94%;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  background-color: #EFEFEF;
  border-top: 5px solid #F5F5F5;
  margin-bottom: 10px;
}

.note1 {
  margin-left: 15px;
  margin-right: 15px;
  width: 500rpx;
  height: 60px;
  font-size: 14px;
  margin-bottom: 50px;
}

.bottomView {
  height: 50px;
  background-color: #FFFFFF;
  display: flex;
  position: fixed;
  justify-content: space-between;
  align-items: center;
  left: 0;
  bottom: 0;
  width: 100%;
  border-top: 1px solid #E9E9E9;
}

.bottomView .priceDes {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.bottomView .priceDes .title {
  font-size: 20px;
  color: #FF4242;
}

.bottomView .priceDes .subtitle {
  font-size: 12px;
  color: #aaa;
  margin-left: 5px;
}

.bottomText1 {
  color: white;
  margin-left: 15px;
  font-size: 14px;
}

.commigBt {
  background-color: #FFD328;
  color: #353123;
  font-size: 16px;
  height: 50px;
  width: 100px;
  line-height: 50px;
  text-align: center;
}

.chooseTimeView {
  margin-left: 15px;
  margin-right: 15px;
  font-size: 12px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.chooseTimeView .newnote {
  margin-bottom: 6px;
  font-size: 16px;
}

.pickerShow { 
  color: #303030;
  font-size: 14px;
  border-radius: 5px;
  border: solid #ffd33a 2rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.chooseTimeItem {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-bottom: 10px;
  align-items: flex-start;
}

.chooseTime {
  margin: 0px 10px;
  padding: 4px 10px;
  font-size: 14px;
  border: 1px solid #ececec;
  border-radius: 2px;
}
.bottomLeft{
  display: flex;
  flex-direction: row;
  width: 40%;
  justify-content: space-around;
  align-items: center;
  margin-left: 15px;
}
.bottomLeft>image{
  width: 35px;
}

.nulladdress{
  width: 90%;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
}
.nulladdress:first-of-type{
  border-bottom: 1px solid #EDEDED;
}
.nulladdress>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.nulladdress>view:first-of-type>image{
  width: 70rpx;
}
.nulladdress>view:last-of-type>image{
  width: 40rpx;
}
.nulladdress>view>text{
  font-size: 28rpx;
}
.nulladdress>view:nth-of-type(2){
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.last{
  font-size: 24rpx;
  color: #7C7C7C;
}
.itemicon{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #FFD428;
  padding: 3px;
}
.itemicon>image{
  width: 28px;
}
.time{
  padding: 3px 5px;
  font-size: 12px;
}
.chooseTimeItem>view:first-of-type{
  margin: 5px 0;
}
.error{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 15px 0;
  padding-left: 15px;
  border-bottom: 5px solid #EFEFEF;
}
.error>image{
  width: 15px;
  vertical-align: middle;
}
.error>text{
  font-size: 12px;
  color: #808080;
}
.shopname{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1px solid #ECECEC;
}
.shopname>image{
  vertical-align: middle;
}
.countsline{
  height: 86rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.countsline view{
  color: #FFD328;
  font-size: 36rpx;
  margin: 0 20rpx 0 20rpx
}
.countsline text{
  color: #333;
  font-size: 32rpx;
}