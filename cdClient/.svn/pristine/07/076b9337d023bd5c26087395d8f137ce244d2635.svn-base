<!--pages/psdetails/psdetails.wxml-->
<view class='containter'>
  <view class='header'>
    <view>
      <image src='/images/ic_courier.png' mode='widthFix'></image>
      <view>
        <text>李师傅</text>
        <text>15295863625</text>
      </view>
    </view>
    <image src='/images/btn_express_telephone.png' mode='widthFix' bindtap='gotel' data-tel='15295863625'></image>
  </view>
  <view class='kdmessage'>
    <view>
      <image src='/images/ic_transport.png' mode='widthFix'></image>
      <text>当前配送信息</text>
    </view>
    <view>
      <view class='line'>
        <view></view>
        <view></view>
        <view></view>
      </view>

      <view>
        <text>接到宠物</text>
        <image src='/images/btn_fine_green_selected.png' mode='widthFix'></image>
      </view>
      <view>
        <text>送达商家</text>
        <image src='/images/btn_fine_green_selected.png' mode='widthFix'></image>
      </view>
      <view>
        <text>送宠回家</text>
        <image src='/images/btn_gray_normal_three.png' mode='widthFix'></image>
      </view>
      <view>
        <text>完成订单</text>
        <image src='/images/btn_gray_normal_four.png' mode='widthFix'></image>
      </view>
    </view>
  </view>
  <view class='map'>
    <text>查看宠物距离</text>
    <view>
      <map id="map" longitude="113.324520" latitude="23.099994" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location>

      </map>
    </view>
    
  </view>
</view>
