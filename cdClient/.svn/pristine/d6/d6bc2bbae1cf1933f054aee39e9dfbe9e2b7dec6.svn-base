.containter{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.list{
  display: flex;
  flex-direction: column;
}
.list>view{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 94%;
  padding: 0 3%;
  height: 55px;
  background-color: #ffffff;
  border-bottom: 1px solid #EEEEEE;
}
.list>view>view:first-of-type{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.list>view>view:first-of-type>text:first-of-type{
  font-size: 18px;
  color: #292929;
}
.list>view>view:first-of-type>text:last-of-type{
  font-size: 12px;
  color: #585858;
}
.list>view>view:last-of-type>text{
  font-size: 22px;
}

