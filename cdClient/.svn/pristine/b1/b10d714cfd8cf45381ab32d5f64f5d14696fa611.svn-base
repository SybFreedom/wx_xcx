.containter{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.containterTop{
  width: 100%;
  height: 250rpx;
  background-color: #FFD428;
  display: flex;
  justify-content: center;
  align-items: center;
}
.containterTop>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.containterTop>view>text:first-of-type{
  font-size: 14px;
  color: #2F2C22;
}
.containterTop>view>text:last-of-type{
  font-size: 24px;
  color: #222222;
}
.containterCenter{
  display: flex;
  flex-direction: column;
}
.top>view:first-of-type{
  width: 94%;
  padding: 0 3%;
  background-color: #EFEFEF;
  height: 40px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.top>view:first-of-type>text:first-of-type{
  font-size: 36rpx;
  color: #3F3F3F;
}
.top>view:first-of-type>text:last-of-type{
  font-size: 28rpx;
  color: #3E3E3E;
}
.top>view:nth-of-type(2){
  width: 94%;
  padding: 0 3%;
  background-color: #ffffff;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.top>view:nth-of-type(2)>input{
  width: 100%;
  border: none;
  height: 45px;
  font-size: 14px;
}
.top>view:nth-of-type(2)>view{
  width: 25%;
  height: 80rpx;
  /* background-color: #CECECE; */
  color: #252422;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  font-size: 32rpx;
}
.top>view:nth-of-type(3){
  width: 94%;
  padding: 0 3%;
  height: 60px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.top>view:nth-of-type(3)>image{
  width: 40px;
  margin-right: 5px;
}
.top>view:nth-of-type(3)>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.top>view:nth-of-type(3)>view>text:first-of-type{
  font-size: 16px;
  color: #2D2D2D;
}
.top>view:nth-of-type(3)>view>text:last-of-type{
  font-size: 12px;
  color: #A0A0A0;
}
.center{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.center>view{
  width: 94%;
  padding: 0 3%;
  background-color: #ffffff;
  border-bottom: 1px solid #F0F0F0;
  height: 70px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.center>view>view:first-of-type{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.center>view>view:first-of-type>text:first-of-type{
  font-size: 20px;
  color: #222222;
}
.center>view>view:first-of-type>text:last-of-type{
  font-size: 14px;
  color: #FB4158;
}
.center>view>view:last-of-type{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #FFD141;
  width: 20%;
  height: 40px;
  border-radius: 5px;
}
.center>view>view:last-of-type>text{
  font-size: 16px;
}
.bottom{
  width: 94%;
  padding: 0 3%;
  margin-top: 10px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.bottom>image{
  width: 15px;
  margin-right: 5px;
}
.bottom>text{
  font-size: 12px;
  color: #989898;
}