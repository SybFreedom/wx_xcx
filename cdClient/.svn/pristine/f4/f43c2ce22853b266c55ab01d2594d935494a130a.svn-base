
page{
  height: 100%;
}
.secitonView{
  background-color: #fff;
}
image{height:auto}
.headerView{
  display: flex;
  padding-left: 10px;
  padding-top: 10px;
  padding-right: 10px;
  padding-bottom: 10px;
  justify-content: space-between;
  align-items: center;
  font-size: 12px;
  flex-direction: row;
  border-bottom: 1px solid #ececec;
}

.headerLeftView{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.headerRightView>image{
  width: 8px;
}
.headerIcon{
  width: 30px;
  height: 30px;
  border-radius: 50%;
  margin-right: 20rpx;
}

.headerName{
  margin-left: 6px;
  margin-bottom: 6px;
}

.headerOrderNum{
  margin-left: 6px;
}

.headerOrderState{
  color: #ffd33a;
}

.centerView{
  width: 94%;
  margin: 0 3%;
  border-bottom: 1px solid #ececec;
}

.goodsListView{
  margin-bottom: 1px;
  padding-top: 10px;
  padding-bottom: 10px;
  /*background-color: lavenderblush;*/
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 12px;
}
.goodsItemLeftView>image{
  width: 70px;
  min-height: 70px;
  border-radius: 3px;
  margin-right: 10rpx;
}

.nameAndOrderNumber{
  display: flex;
  flex-direction: column;
}

.goodsName{
  margin-left: 10px;
}

.goodsItemRightView{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.goodsItemRightView>text:first-of-type{
  font-size: 12pt;
  color: #727272;
}
.goodsItemRightView>text:nth-of-type(2){
  font-size: 11pt;
  color: #FA4157;
}
.goodsItemRightView>text:nth-of-type(3){
  font-size: 10pt;
  color: #979797;
}

.goodsNumber{
  margin-right: 10px;
  text-indent: 0.3rem;
}

.goodsSingPric{
  margin-right: 10px;
}

.footerView{
  width: 94%;
  margin: 0 3%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 10px;
  border-top: 1px solid #E9E9E9;
}

.leftView{
  font-size: 12px;
  display: flex;
  flex-direction: row;
}

.rightView{
  display: flex;
}
.rightView>text:first-of-type{
  font-size: 10pt;
  color: #FF7070;
  line-height: 32px;
}

.serviceAndPrice{
  margin-bottom: 6px;
}

.footerViewBt{
  margin-bottom: 10px;
  margin-top: 10px;
  margin-right: 6px;
  padding-left: 12px;
  padding-right: 12px;
  padding-bottom: 6px;
  padding-top: 6px;
  font-size: 12px;
  color: #606060;
  border-radius: 18px;
  border:1px solid #DBDBDB;
  background-color: #EFEFEF;
}
.emptyOrder{
  display: flex;
  flex: 1;
  height: 100%;
  justify-content: center;
  align-items: flex-start;
  background-color: #ffffff;
}
.emptyOrder>view{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  margin-top: 300rpx;
}
.emptyOrder>view>image{
  width: 300rpx;
  margin-bottom: 20rpx;
}
.emptyOrder>view>text{
  font-size: 14px;
  color: #B6B6B6;
}
.ordernumber{
  width: 94%;
  margin: 0 3%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx solid #EAEAEA;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.ordernumber>text{
  font-size: 12px;
}
.ordernumber>text:first-of-type{
  color: #BBBBBB;
}
.ordernumber>text:last-of-type{
  color: #FFE27E;
}
.ordertime{
  width: 94%;
  padding: 0 3%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  margin: 10rpx 0;
}
.ordertime>text{
  font-size: 9pt;
  color: #A7A7A7;
  line-height: 40rpx;
}
.message{
  display: flex;
  flex-direction: column;
  width: 94%;
  padding: 0 3% 20rpx;
}
.message>text:first-of-type{
  font-size: 12pt;
  color: #333;
  font-weight: bold;
}
.message>text:last-of-type{
  font-size: 10pt;
  color: #979797;
}
.rightView>text:nth-of-type(2){
  background-color: #FF4242;
  color: #ffffff;
  font-size: 10pt;
  border-radius: 18px;
  padding-left: 12px;
  padding-right: 12px;
  padding-bottom: 6px;
  padding-top: 6px;
}