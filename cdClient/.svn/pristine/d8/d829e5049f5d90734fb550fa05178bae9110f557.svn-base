page{
  height: 100%;
  background-color: #F3F3F3;
}
.containter{
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
}
.content{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 1;
  margin-top: 50rpx;
}
.content>image{
  width: 170rpx;
  height: 170rpx;
  border-radius: 50%;
}
.content>text{
  margin: 30rpx 0;
  font-size: 16px;
}
.content>view{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ffffff;
}
.content>view>text{
  font-size: 14px;
  color: #666666;
}