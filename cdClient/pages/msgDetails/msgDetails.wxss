/* pages/serviceDes/serviceDes.wxss */
page{
  background-color: white;
}
.container{
  display: flex;
  flex-direction: column;
  padding: 0 2%;
}
.container>text:first-of-type{
  font-size: 14pt;
  color: #333333;
  text-align: center;
  margin-top: 10rpx;
}
.container>text:nth-of-type(2){
  font-size: 9pt;
  color: #999999;
  text-align: center;
  margin: 10rpx 0;
}
.container>text:nth-of-type(3){
  font-size: 10pt;
  color: #666666;
  text-indent: 2em;
}
.container>text:last-of-type{
  font-size: 9pt;
  color: #999999;
  text-align: right;
  margin: 10rpx 0;
  float: right;
}
