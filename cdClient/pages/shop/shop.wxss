}

.shopInfoLine {
  width: 670rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  height: 60rpx;
}

.shopInfoLine>text:nth-of-type(1) {
  font-size: 28rpx;
  color: #64363c;
  line-height: 60rpx;
  float: left;
}
.shopInfoLine>text:nth-of-type(2) {
  font-size: 24rpx;
  color: #64363c;
  line-height: 60rpx;
  float: right;
}
