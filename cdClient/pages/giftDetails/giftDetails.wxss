.container{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.container>text{
  font-size: 12pt;
  color: #333333;
  font-weight: bold;
  width: 94%;
  margin: 20rpx auto 0;
  border-bottom: 1rpx solid #E6E6E6;
  padding-bottom: 10rpx;
}
.details{
  width: 90%;
  padding: 30rpx 2%;
  margin: 20rpx auto;
  height: auto;
  background-color: #FFFFFF;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  border-radius: 5rpx;
}
.details>view{
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}
.details>view>view:first-of-type{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 7rpx;
}
.details>view>view:first-of-type>image{
  width: 30rpx;
  margin-right: 10rpx;
}
.details>view>view:first-of-type{
  width: 27%;
}
.details>view>view:first-of-type>text{
  font-size: 11pt;
  color: #707070;
}
.details>view>view:last-of-type{
  width: 73%;
}
.details>view>view:last-of-type{
  line-height: 50rpx;
}
.details>view>view:last-of-type>text{
  font-size: 10pt;
  color: #787878;
  word-break: break-all
  
}
.gosubmit{
  width: 300rpx;
  height: 80rpx;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #FFD428;
  margin: 50rpx auto;
}
.gosubmit>text{
  font-size: 13pt;
}


.wrap{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.5);
  position: fixed;
  top: 0;
  left: 0;
}
.wrap>view:first-of-type{
  width: 100%;
  height: 67%;
}
.wrap>view:last-of-type{
  width: 100%;
  height: 33%;
  background-color: #ffffff;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
}
.wrap>view:last-of-type>view:first-of-type{
  width: 94%;
  margin: 50rpx auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.wrap>view:last-of-type>view:first-of-type>view{
  width: 230rpx;
  height: 2rpx;
  background-color: #E2E2E2;
}
.wrap>view:last-of-type>view:first-of-type>text{
  font-size: 14px;
  color: #757575;
}

.wrap>view:last-of-type>view:nth-of-type(2){
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.wrap>view:last-of-type>view:nth-of-type(2)>image{
  width: 80rpx;
}
.wrap>view:last-of-type>view:nth-of-type(2)>text{
  font-size: 20px;
  color: #333;
  margin: 0 30rpx;
}
.wrap>view:last-of-type>view:last-of-type{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid #E6E6E6;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}
.wrap>view:last-of-type>view:last-of-type>view:first-of-type{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-left: 3%;
}
.wrap>view:last-of-type>view:last-of-type>view:first-of-type>image{
  width: 70rpx;
  margin-right: 10rpx;
}
.wrap>view:last-of-type>view:last-of-type>view:first-of-type>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  
}
.wrap>view:last-of-type>view:last-of-type>view:first-of-type>view>text:first-of-type{
  font-size: 20px;
  color: #FF4242;
  line-height: 20px;
}
