/**index.wxss**/
page{
  background-color: #F5F5F5;
}
.container{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.nullcar{
  width: 50%;
  height: auto;
  overflow: hidden;
  margin: 30px auto 0;
}
.nullcar>image{
  width: 50%;
  margin: 0 auto;
  display: block;
}
.nullcar>text{
  display: block;
  text-align: center;
  margin-top: 10px;
  font-size: 11pt;
  color: #999999;
  margin-bottom: 30px;
}
.nullcar>navigator{
  width: 100px;
  height: 40px;
  background-color: #F0E156;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ffffff;
  border-radius: 5px;
  margin: 0 auto;
}
.nullcar>navigator>text{
  font-size: 10pt;
}
.shoptlistitem{
  margin-bottom: 120rpx;
}
.item{
  width: 90%;
  padding: 20px 5%;
  border-top: 1px solid #f2f2f2;
  height: auto;
  overflow: hidden;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: center;
}
.selecticon{
  width: 20px;
}
.itemright{
  display: flex;
  width: 85%;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.itemright>navigator{
  width: 30%;
}
.itemright>navigator>image:first-of-type{
  width: 100%;
}
.itemright>view{
  width: 65%;
  
}
.itemright>view>navigator{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: space-between;
}
.itemright>view>navigator>text{
  font-size: 10pt;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.itemright>view>navigator>text:nth-of-type(2){
  color: #A1A1A1;
}
.itemright>view>view{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 30px;
}
.itemright>view>view>text{
  font-size: 11pt;
  color: #D45149;
}
.itemright>view>view>view{
  display: flex;
  flex-direction: row;
}
.itemright>view>view>view>text{
  font-size: 10pt;
  color: #676767;
}
.itemright>view>view>view>text:first-of-type{
  width: 25px;
  height: 25px;
  text-align: center;
  line-height: 25px;
  border: 1rpx solid #E3E3E3;
}
.itemright>view>view>view>text:nth-of-type(2){
  width: 40px;
  height: 25px;
  text-align: center;
  line-height: 25px;
  border: 1rpx solid #E3E3E3;
  margin-left: -1px;
}
.itemright>view>view>view>text:last-of-type{
  width: 25px;
  height: 25px;
  text-align: center;
  line-height: 25px;
  border: 1rpx solid #E3E3E3;
  margin-left: -1px;
}
.all{
  display: flex;
  width: 100%;
  height: 55px;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #ffffff;
  border-top: 1px solid #f1f1f1;
}
.all>view:first-of-type{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-left: 5%;
}
.all>view:first-of-type>image{
  width: 20px;
  margin-right: 5px;
}
.all>view:first-of-type>text{
  font-size: 11pt;
  color: #686868;
}
.all>view:nth-of-type(2){
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 70%;
}
.all>view:nth-of-type(2)>text{
  font-size: 11pt;
  color: #D45149;
}
.all>view:nth-of-type(2)>navigator{
  width: 50%;
  background-color: #FFD33A;
  height: 55px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.all>view:nth-of-type(2)>navigator>text{
  font-size: 11pt;
  color: #ffffff;
}
