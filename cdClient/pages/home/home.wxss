.guideWrap {
  background-color: rgba(0, 0, 0, 0.7);
  position: fixed;
  z-index: 99998;
  display: flex;
  flex-direction: column;
  /* justify-content: center;
  align-items: center; */
}

.guideWrap>view:first-of-type {
  width: 100%;
  height: 400rpx;
  position: relative;
  /* background-color: red; */
}

.guideWrap>view:first-of-type>view {
  width: 400rpx;
  border: 2rpx solid #fff;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
  position: absolute;
  left: 45rpx;
  top: 90rpx;
  animation: txt 1s;
}

@keyframes txt {
  from {
    top: -90rpx;
  }

  to {
    top: 90rpx;
  }
}

.guideWrap>view:first-of-type>view>text {
  color: #fff;
  font-size: 11pt;
  line-height: 50rpx;
}

button::after {
  border: none;
}

.guideWrap>view:first-of-type>image {
  width: 160rpx;
  position: absolute;
  right: 17%;
  top: 0;
  animation: up 1s;
}

@keyframes up {
  from {
    right: -17%;
  }

  to {
    right: 17%;
  }
}

.guideWrap>view:first-of-type>button {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: transparent;
  color: #fff;
  border: 2rpx solid #fff;
  border-radius: 50rpx;
  font-size: 12pt;
  position: absolute;
  left: 300rpx;
  top: 250rpx;
  animation: next 1s;
}

@keyframes next {
  from {
    top: 500rpx;
  }

  to {
    top: 250rpx;
  }
}

.guideWrap>view:nth-of-type(2) {
  width: 100%;
  height: 500rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  /* background-color: red; */
}

.guideWrap>view:nth-of-type(2)>view {
  width: 400rpx;
  border: 2rpx solid #fff;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
  position: absolute;
  right: 25rpx;
  bottom: 170rpx;
  animation: giftTxt 1s;
}

@keyframes giftTxt {
  from {
    bottom: 400rpx;
  }

  to {
    bottom: 170rpx;
  }
}

.guideWrap>view:nth-of-type(2)>view>text {
  color: #fff;
  font-size: 11pt;
  line-height: 50rpx;
}

.guideWrap>view:nth-of-type(2)>button {
  padding: 0;
  margin: 0;
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: transparent;
  color: #fff;
  border: 2rpx solid #fff;
  border-radius: 50rpx;
  font-size: 12pt;
  position: absolute;
  left: 300rpx;
  bottom: 80rpx;
  animation: know 1s;
}

@keyframes know {
  from {
    bottom: -80rpx;
  }

  to {
    bottom: 80rpx;
  }
}

.guideWrap>view:nth-of-type(2)>image {
  width: 130rpx;
  position: absolute;
  bottom: 0;
  right: 5%;
  animation: down 1s;
}

@keyframes down {
  from {
    right: -5%;
  }

  to {
    right: 5%;
  }
}

