/* 隐藏scrollbar */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.view-parent {
  width: 300rpx;
  height: auto;
  overflow: hidden;
  display: inline-block;
  margin-right: 15rpx;
}

.view-parent:last-of-type {
  margin-right: 45rpx;
}

.view-parent>view {
  box-sizing: border-box;
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  border: 1rpx solid #eaeaea;
}

.view-parent>view>image {
  width: 100%;
  height: 200rpx;
}

.view-parent>view>text {
  width: 95%;
  margin: 0 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.view-parent>view>text:first-of-type {
  font-size: 11pt;
  color: #222;
}

.view-parent>view>text:nth-of-type(2) {
  font-size: 10pt;
  color: #ffd33a;
  line-height: 30rpx;
}

.view-parent>view>text:nth-of-type(3) {
  font-size: 9pt;
  color: #5e5e5e;
}

.view-parent>view>view {
  width: 95%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: auto;
  overflow: hidden;
}

.view-parent>view>view text {
  font-size: 8pt;
}

.view-parent>view>view>text {
  margin-top: 8rpx;
}

