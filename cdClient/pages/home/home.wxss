.cardWrap {
  background-color: rgba(0, 0, 0, 0.5);
  position: fixed;
  z-index: 99999;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cardWrap>view {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.cardWrap>view>image:first-of-type {
  width: 50%;
  position: absolute;
  left: 25%;
  bottom: 150rpx;
}

.cardWrap>view>image:last-of-type {
  width: 80rpx;
